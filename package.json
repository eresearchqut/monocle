{
  "name": "@eresearchqut/future-state-mono-repo",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/eresearchqut"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:eresearchqut/future-state-mono-repo.git",
    "directory": "@eresearchqut/future-state-mono-repo"
  },
  "scripts": {
    "clean": "turbo run clean && rimraf storybook-static && rimraf node_modules",
    "build": "yarn turbo run build",
    "build:no-cache": "turbo run build --no-cache --parallel --continue",
    "lint": "yarn turbo run lint",
    "storybook": "start-storybook",
    "storybook:build": "build-storybook",
    "test": "yarn turbo run test",
    "changeset": "changeset",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=docs^... && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "6.4.22",
    "@storybook/storybook-deployer": "^2.8.11",
    "@types/jest": "26.0.24",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "depcheck": "^1.4.3",
    "eslint": "7.32.0",
    "eslint-config-eslint": "7.0.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "7.25.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "is-ci": "3.0.0",
    "jest": "26.6.0",
    "prettier": "2.6.2",
    "primereact": "8.0.1",
    "rimraf": "3.0.2",
    "sass-loader": "10.2.1",
    "storybook-stylesheet-toggle": "^0.1.3",
    "ts-jest": "26.5.5",
    "turbo": "^1.2.9",
    "typescript": "4.3.5"
  }
}
