{
  "name": "@eresearchqut/future-state-mono-repo",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/eresearchqut"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:eresearchqut/future-state-mono-repo.git",
    "directory": "@eresearchqut/future-state-mono-repo"
  },
  "scripts": {
    "clean:modules": "find . -type d -name node_modules -exec rm -rf {} \\;",
    "clean:dist": "find . -type d -name dist -exec rm -rf {} \\;",
    "clean:turbo": "find . -type d -name .turbo -exec rm -rf {} \\;",
    "clean": "(yarn clean:dist) && (yarn clean:turbo) && (yarn clean:modules)",
    "storybook": "start-storybook",
    "storybook-build": "build-storybook",
    "build": "yarn turbo run build",
    "test": "yarn turbo run test"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "6.4.22",
    "@storybook/storybook-deployer": "^2.8.11",
    "@types/jest": "26.0.24",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "depcheck": "^1.4.3",
    "eslint": "7.32.0",
    "eslint-config-eslint": "7.0.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-react": "7.25.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "is-ci": "3.0.0",
    "jest": "26.6.0",
    "prettier": "2.6.2",
    "primereact": "8.0.1",
    "rimraf": "3.0.2",
    "sass-loader": "10.2.1",
    "storybook-stylesheet-toggle": "^0.1.3",
    "ts-jest": "26.5.5",
    "turbo": "^1.2.9",
    "typescript": "4.3.5"
  }
}
