{
  "name": "@eresearchqut/form-compiler",
  "version": "0.25.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "directory": "packages/form-compiler",
    "url": "ssh://git@github.com:eresearchqut/future-state-mono-repo.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "prepack": "yarn build",
    "build": "yarn clean && tsc --build && cp \"./package.json\" ./dist/",
    "test": "jest --no-cache",
    "test-cov": "jest --no-cache --coverage",
    "storybook": "echo 'no storybook'",
    "build-storybook": "echo 'build-storybook'",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=0 --fix"
  },
  "dependencies": {
    "@eresearchqut/form-components": "^0.25.0",
    "@eresearchqut/form-definition": "^0.25.0",
    "@jsonforms/core": "^3.0.0-alpha.1",
    "cldr-core": "^39.0.0",
    "inversify": "5.0.1",
    "lodash": "^4.14.171",
    "reflect-metadata": "0.1.13",
    "typescript-json-schema": "0.50.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/react": "16.9.48",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
