{
  "name": "@eresearchqut/form-components",
  "version": "0.32.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "directory": "packages/form-components",
    "url": "ssh://git@github.com:eresearchqut/future-state-mono-repo.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "copy-files": "copyfiles -u 1 \"src/**/*.scss\" dist/ && copyfiles -u 1 \"src/**/*.svg\" dist/ && copyfiles package.json dist/",
    "prepack": "yarn build",
    "svgson": "svgson src/component/SvgMap/svgs --output src/component/SvgMap/maps/svgs",
    "build": "yarn clean && yarn svgson && tsc --build && yarn copy-files",
    "test": "jest --no-cache",
    "test-cov": "jest --no-cache --coverage",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --fix --max-warnings=0"
  },
  "dependencies": {
    "@jsonforms/core": "3.0.0-alpha.3",
    "@jsonforms/react": "3.0.0-alpha.3",
    "country-region-data": "^1.11.0",
    "easymde": "2.15.0",
    "lodash": "4.17.21",
    "primeflex": "2.0.0",
    "primeicons": "5.0.0",
    "primereact": "8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-img-mapper": "1.2.2",
    "react-resize-detector": "^6.7.6",
    "react-simplemde-editor": "5.0.2",
    "react-transition-group": "^4.4.1",
    "signature_pad": "^4.0.0",
    "trim-canvas": "^0.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@types/enzyme": "^3.10.3",
    "@types/lodash": "4.14.171",
    "@types/react": "17.0.16",
    "@types/uuid": "^8.3.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "copyfiles": "2.4.1",
    "enzyme": "^3.10.0",
    "jsdom": "^16.6.0",
    "react-redux": "^7.2.4",
    "svgson-cli": "^1.1.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.tsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
