{
  "name": "@eresearchqut/form-designer",
  "version": "0.19.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "directory": "packages/form-components",
    "url": "https://github.com/eresearchqut/future-state-mono-repo/tree/main/packages/form-designer"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "copy-files": "copyfiles -u 1 src/**/*.scss dist/ && copyfiles package.json dist/",
    "prepack": "yarn build",
    "build": "yarn clean && tsc --build && yarn copy-files",
    "test": "jest --no-cache",
    "test-cov": "jest --no-cache --coverage",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=0 --fix",
    "storybook": "(start-storybook -p 6007)",
    "build-storybook": "rimraf storybook-static && build-storybook -o ../../storybooks/form-designer"
  },
  "dependencies": {
    "@eresearchqut/form-compiler": "^0.19.0",
    "@eresearchqut/form-components": "^0.19.0",
    "@eresearchqut/form-definition": "^0.19.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.15",
    "react-beautiful-dnd": "13.1.0",
    "react-json-editor-ajrm": "2.5.13",
    "react-stickynode": "^4.0.0",
    "sass": "^1.37.5",
    "typescript-json-schema": "^0.50.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.9",
    "@storybook/storybook-deployer": "^2.8.10",
    "@svg-maps/cli": "^1.0.0-beta.5",
    "@types/enzyme": "^3.10.3",
    "@types/lodash": "4.14.171",
    "@types/react": "17.0.16",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "17.0.9",
    "@types/react-stickynode": "^4.0.0",
    "@types/uuid": "^8.3.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-loader": "^8.2.2",
    "copyfiles": "2.4.1",
    "enzyme": "^3.10.0",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.3.2",
    "react-redux": "^7.2.4",
    "storybook-stylesheet-toggle": "^0.1.3",
    "typescript-plugin-css-modules": "3.4.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.tsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
