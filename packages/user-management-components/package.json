{
  "name": "@eresearchqut/user-management-components",
  "version": "0.22.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "directory": "packages/user-management-components",
    "url": "ssh://git@github.com:eresearchqut/future-state-mono-repo.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "prepack": "yarn build",
    "build": "yarn clean && tsc --build",
    "test": "jest --no-cache",
    "test-cov": "jest --no-cache --coverage",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --fix --max-warnings=0",
    "storybook": "(start-storybook -p 6008)",
    "build-storybook": "rimraf storybook-static && build-storybook -o ../../storybooks/user-manager",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "primeflex": "2.0.0",
    "primeicons": "4.1.0",
    "primereact": "6.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@storybook/addon-actions": "^6.4.8",
    "@storybook/addon-essentials": "^6.4.8",
    "@storybook/addon-links": "^6.4.8",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.8",
    "@storybook/storybook-deployer": "^2.8.10",
    "@svgr/cli": "^6.1.1",
    "@svgr/plugin-jsx": "^6.1.0",
    "@types/enzyme": "^3.10.3",
    "@types/lodash": "4.14.171",
    "@types/react": "17.0.16",
    "@types/react-dom": "17.0.9",
    "@types/uuid": "^8.3.1",
    "babel-loader": "^8.2.2",
    "eslint-config-prettier": "^8.3.0",
    "react-redux": "^7.2.4",
    "storybook-stylesheet-toggle": "^0.1.3",
    "svgson-cli": "^1.1.1",
    "typescript-plugin-css-modules": "3.4.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.tsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
