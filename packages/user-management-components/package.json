{
  "name": "@eresearchqut/user-management-components",
  "version": "0.33.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "directory": "packages/user-management-components",
    "url": "ssh://git@github.com:eresearchqut/future-state-mono-repo.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "prepack": "yarn build",
    "build": "yarn clean && tsc --build",
    "test": "echo '(add some tests) jest --no-cache'",
    "test-cov": "echo '(add some tests) jest --no-cache --coverage'",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --fix --max-warnings=0"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "primereact": "8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@types/lodash": "4.14.171",
    "@types/react": "17.0.16"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.tsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
