{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./packages",
    "paths": {
      "@eresearchqut/*": [
        "./*/src"
      ]
    },
    "target": "es5",
    "lib": [
      "es2020",
      "dom"
    ],
    "types": [
      "reflect-metadata",
      "jest"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": false,
    "noResolve": false,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "files": [],
  "references": [
    {
      "path": "packages/form-compiler"
    },
    {
      "path": "packages/form-components"
    },
    {
      "path": "packages/form-definition"
    },
    {
      "path": "packages/form-designer"
    },
    {
      "path": "packages/resource-api"
    },
    {
      "path": "packages/tenant-resource-manager"
    },
    {
      "path": "packages/user-management-api"
    },
    {
      "path": "packages/user-management-components"
    }
  ]
}